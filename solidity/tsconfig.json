{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"rootDir": "ts",
		"outDir": "js",
		"sourceMap": false,
		"inlineSources": false,
		"declaration": false,
		"declarationMap": false,
		"noEmit": false,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"lib": ["ESNext", "DOM"],
		"types": ["node", "bun-types"],
		"skipLibCheck": true
	},
	"include": [
		"./ts/**/*.ts",
	]
}
