{
	"version": "1.0.0",
	"type": "module",
	"description": "Zoltar Contracts",
	"scripts": {
		"setup": "bun install --ignore-scripts && bun run compile-contracts",
		"compile-contracts": "bun tsc --project tsconfig-compile.json && bun run ./js/compile.js",
		"test-zoltar": "bun tsc && bun test --timeout 300000 ./js/tests/zoltar.test.js",
		"test-peripherals": "bun tsc && bun test --timeout 300000 ./js/tests/peripherals.test.js",
		"test-escalation-game": "bun tsc && bun test --timeout 300000 ./js/tests/escalationGame.test.js",
		"test-question-data": "bun tsc && bun test --timeout 300000 ./js/tests/questionData.test.js",
		"test-auction": "bun tsc && bun test --timeout 300000 ./js/tests/auction.test.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"funtypes": "5.1.1",
		"solc": "0.8.33",
		"tsx": "4.19.3",
		"typescript": "5.8.2",
		"viem": "2.38.3"
	}
}
